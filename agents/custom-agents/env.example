# SAM Agent Deployment Configuration
# Copy this file to .env and update the values

# SAM Installation Path
# The script will auto-detect common paths, but you can override here
SAM_PATH=/path/to/your/sam/installation

# Default Agent Name (optional)
AGENT_NAME=find_my_ip_agent

# SAM Configuration
SAM_CONFIG_PATH=${SAM_PATH}/configs
SAM_AGENTS_PATH=${SAM_PATH}/src

# Virtual Environment Path (usually auto-detected)
VENV_PATH=${SAM_PATH}/.venv
